{% extends "base_session.html.twig" %}

{% block title %}
Inicio
{% endblock %}

{% block container %}

<div class="container">
<div class="row">
<div class="col-sm-12 col-sm-offset-0 text-center well">
<ul>
{% if viajes %}
	{% for viaje in viajes %}
		<li>
		<h3>{{ viaje.origen }}</h3> <h3>{{ viaje.destino }}</h3> <h3>{{ viaje.fecha }}</h3>
		</li>
	{% endfor %} 

{% else %}
	<h1>No hay viajes para mostrar</h1>
{% endif %}

</ul>
</div>
</div>
</div>

{% endblock %}